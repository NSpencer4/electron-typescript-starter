{
  "name": "project-atlas",
  "version": "1.0.0",
  "description": "A minimal Electron application with three.js",
  "main": "src/main.js",
  "author": "Chase Spencer",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "webpack --config webpack.config.js",
    "serve": "electron ."
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "modulePaths": [
    "<rootDir>/packages/electron-webpack/node_modules",
    "<rootDir>/packages"
  ],
  "dependencies": {
    "electron": "^4.0.1",
    "fsevents": "^1.2.4",
    "jquery": "^3.3.1",
    "html-webpack-plugin": "^3.2.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.29",
    "@types/node": "^10.12.18",
    "@types/webpack": "^4.4.22",
    "electron-packager": "^13.0.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14"
  }
}
